# ====== LLM (Zhipu / BigModel) ======
LLM_PROVIDER=zhipu
ZHIPU_MODEL=glm-4.7
ZHIPU_BASE_URL=https://open.bigmodel.cn/api/paas/v4/
ZHIPU_API_KEY=

# Compatibility fallback (optional)
OPENAI_API_KEY=

# ====== SQL benchmark DB ======
CHINOOK_DB_PATH=./data/Chinook.db

# ====== MCP chart tool ======
ENABLE_MCP_CHART=1

# ====== Artifact download ======
ENABLE_DOWNLOAD_TOOL=1

# ====== Persistence (required by ag_ui_langgraph for state/history/interrupt) ======
# Stored relative to apps/agui_events_demo2
CHECKPOINTER_PATH=./data/checkpoints.sqlite

# ====== Server Ports ======
PORT=8123
FRONTEND_PORT=8124
