# OpenAI API Key (用于LangGraph代理)
OPENAI_API_KEY=your-openai-api-key-here

# LangSmith API Key (用于追踪和调试LangGraph运行)
LANGSMITH_API_KEY=your-langsmith-api-key-here

# 示例: DeepSeek API Key (如果需要使用DeepSeek模型)
DEEPSEEK_API_KEY=your-deepseek-api-key-here

# 示例: Tavily API Key (用于网络搜索功能)
TAVILY_API_KEY=your-tavily-api-key-here

# 示例: Milvus配置 (用于向量数据库)
MILVUS_URI=http://localhost:19530
MILVUS_COLLECTION_NAME=knowledge_base

# 示例: 嵌入模型配置
EMBEDDING_MODEL=your-embedding-model-name
EMBEDDING_BASE_URL=http://localhost:11434

# LangSmith追踪开关
LANGSMITH_TRACING=true

# LangGraph 部署地址（容器内默认指向后端服务名）
LANGGRAPH_DEPLOYMENT_URL=http://langgraph-api:8000

# LangGraph 镜像名（langgraph build 生成的 tag）
IMAGE_NAME=my-langgraph:latest

# 内置 Postgres/Redis 默认配置（如使用外部服务请修改）
POSTGRES_DB=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
REDIS_URI=redis://langgraph-redis:6379
DATABASE_URI=postgres://postgres:postgres@langgraph-postgres:5432/postgres?sslmode=disable
